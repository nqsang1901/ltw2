        <div class="container">
            <%- include('menu');  -%>
        </div>
    </header>

    <!-- ***** Header Area End ***** -->

    <!-- ***** Features Item Start ***** -->
    <section class="section" id="features">
        <div class="container">
            <div class="row">
                <table class="table">
                    <thead>
                        <tr>
                            <th>STT</th>
                            <th>Tên loại đăng ký</th>
                            <th>Giới thiệu</th>
                            <th>Đăng ký</th>
