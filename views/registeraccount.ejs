                        <tr class="active">
                            <td> 1 </td>
                            <td> Tài khoản thanh toán </td>
                            <td> Giới thiệu thanh toán  </td>
                            <td>
                                <button type="button" onclick="window.location.href='/registeraccount/<%= currentUser.UserId %>/1'"
