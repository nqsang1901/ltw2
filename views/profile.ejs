<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
        href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap"
        rel="stylesheet">
    <title>Thông tin tài khoản</title>
    <link rel="stylesheet" type="text/css" href="../css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../css/font-awesome.css">
    <link rel="stylesheet" href="../css/templatemo-training-studio.css">
</head>

<body>
    <header class="header-area header-sticky">
        <div class="container">
            <%- include('menu');  -%>
        </div>
    </header>
    <section class="section" id="features">
        <div class="container">
            <div class="col-lg-6 offset-lg-3">
                <div class="section-heading">

                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-6 col-md-6">
                    <div class="well well-sm">
                        <div class="row">
                            <h4>Tài khoản thanh toán</h4>
                            <div class="col-sm-6 col-md-8">
                                <h4><b><%= currentUser.UserName %></b></h4>
                                <p>
                                    <!-- Số tài khoản: <%#= account.AccountId %> -->
                                    <br />
                                    Email: </i> <%= currentUser.EmailAddress %>
                                    <br />
                                    Phone: </i> <%= currentUser.CellPhone %>
                                    <!-- Split button -->
                            </div>
                            <h4>Tài khoản tiết kiệm</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    

</body>

</html>