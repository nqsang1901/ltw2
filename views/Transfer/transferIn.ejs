<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="../css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../css/font-awesome.css">
    <link rel="stylesheet" href="../css/templatemo-training-studio.css">
    <title>Internet Banking | Đăng nhập</title>
</head>

<body>
    <header class="header-area header-sticky">
        <div class="container">
            <%- include('../menu');  -%>
        </div>
    </header>
    <section class="section" id="features">
        <div class="container">
            <div class="col-lg-6 offset-lg-3">
                <div class="section-heading">

                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <!-- <div class="panel-heading">
                            <h3 class="panel-title">Please sign in</h3>
                        </div> -->
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <form method="POST" accept-charset="UTF-8" role="form">
                                <fieldset>
                                    <div class="form-group">
                                        <% if(typeof(message) != "undefined") { %>
                                            <p style="color: red;"> <%= message %></p>
                                        <% } %>
                                        <input class="form-control" placeholder="Recipient Account Number" id="AccountId" name="AccountId" type="text">
                                        <input class="form-control" placeholder="Amount Of Money" id="money" name="money" type="text">                    
                                        <input class="form-control" placeholder="Password" id="password" name="password" type="text">
                                        <input class="form-control" placeholder="Content" id="content" name="content" type="text">
                                        <input class="btn btn-lg btn-success btn-block" type="submit" value="Gửi ngay">
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>

</body>

</html>